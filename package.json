{
  "name": "ripple-next",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "test:ui": "turbo test:ui",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "typecheck": "turbo typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "turbo clean && rimraf node_modules",
    "db:generate": "pnpm --filter @ripple/db db:generate",
    "db:migrate": "pnpm --filter @ripple/db db:migrate",
    "db:seed": "pnpm --filter @ripple/db db:seed",
    "db:studio": "pnpm --filter @ripple/db db:studio",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@storybook/vue3": "^10.2.13",
    "@storybook/vue3-vite": "^10.2.13",
    "@types/node": "^22.12.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "storybook": "^10.2.13",
    "turbo": "^2.3.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^9.4.3"
  }
}
